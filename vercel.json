{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": "vite",
  "github": {
    "enabled": false
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE": "@vite_api_base"
  }
}
